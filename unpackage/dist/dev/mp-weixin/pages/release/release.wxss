
.outer{
	width: 100%;
	height: 100%;
	position: relative;
}
.outbox{
	width: 70%;
	height: 30%;
	position: absolute;
	top: 60%;
	left: 30%;
}
.tui{
	width: 12%;
	height: 8%;
	text-align: center;
	position: fixed;
	bottom: 20px;
	left: 45%;
	background: #2ED8C4;
	color: #eeeeee;
	outline: none;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	font-size: 40px;
	line-height: 45px;
}
.box{
	width: 75px;
	height: 75px;
	background: #2ED8C4;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.box>image{
	width: 80%;
	height: 80%;
	position: relative;
	top: 10px;
	left: 8px;
}
.leftBox{
	position: absolute;
	top: 60%;
	left: 15%;
}
.rightBox{
	position: absolute;
	top: 60%;
	left: 45%;
}

